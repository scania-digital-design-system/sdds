{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "rules": {
    "no-trailing-spaces": "off", //TODO: turn on
    "max-len": "off", //TODO: Set limit ?
    "linebreak-style": "off",
    "no-use-before-define": "off",
    "import/no-extraneous-dependencies": "off", //TODO: Turn this rule on
    "import/prefer-default-export": "off",
    "arrow-parens": "off", //TODO: Turn on
    "no-undef": "off",
    "no-underscore-dangle": "off", //TODO: turn this rule on
    "import/no-unresolved": "off", //TODO: check if we can use plugin for this rule
    "no-param-reassign": "off", // check this[ "off", { "props": false }]
    "no-return-assign": "off", //TODO: Turn on this rule
    "class-methods-use-this": "off", //TODO: Check if this can be turned on
    "array-callback-return": "off", //TODO: Check this if needed ["warn", { "checkForEach": false }]
    "no-loop-func": "warn", // check this
    "no-self-assign": "warn", // check this
    "consistent-return": "off",
    "no-unused-expressions": "off", //TODO: Turn on ?
    "no-restricted-syntax": "off", //TODO: check this - disables for-in related error
    "import/no-dynamic-require": "warn",
    "func-names": "off", //TODO: Turn this on? you shouldn't use unnamed functions
    "no-console": "off",
    "no-useless-escape": "warn",
    "prefer-destructuring": "off", //TODO: Turn this on?
    "comma-dangle": "off", //TODO: Turn on
    "object-curly-spacing": "off", //TODO: Turn on
    "semi": "off", //TODO: turn on
    "object-shorthand": "warn",
    "space-before-blocks": "off",
    "key-spacing": "off",
    "keyword-spacing": "off",
    "import/extensions": "off",
    "quotes": "off",
    "space-infix-ops": "off",
    "object-curly-newline": "off",
    "spaced-comment": "off",
    "comma-spacing": "off",
    "arrow-spacing": "off",
    "indent": "off",
    "dot-notation": "off",
    "eqeqeq": "off",
    "operator-linebreak": "off",
    "padded-blocks": "off",
    "no-multiple-empty-lines": "off",
    "eol-last": "off",
    "space-before-function-paren": "off",
    "lines-between-class-members": "off",
    "implicit-arrow-linebreak": "off",
    "prefer-arrow-callback": "off",
    "arrow-body-style": "off",
    "prefer-template": "off",
    "no-var": "off", //TODO: Turn on?
    "import/order": "off",
    "array-bracket-spacing": "off",
    "prefer-const": "off",
    "no-extra-semi": "off",
    "camelcase": "off",
    "template-curly-spacing": "off",
    "no-plusplus": "off",
    "function-paren-newline": "off",
    "wrap-iife": "off",
    "no-empty-pattern": "off",
    "no-multi-spaces": "off",
    "import/newline-after-import": "off",
    "vars-on-top": "off",
    "no-shadow": "off",
    "block-scoped-var": "off",
    "brace-style": "off",
    "no-array-constructor": "off",
    "prefer-object-spread": "off",
    "no-nested-ternary": "off",
    "no-confusing-arrow": "off",
    "no-redeclare": "off",
    "block-spacing": "off",
    "space-in-parens": "off",
    "semi-spacing": "off",
    "no-restricted-globals": "off",
    "no-prototype-builtins": "off",
    "curly": "off",
    "no-continue": "off",
    "object-property-newline": "off",
    "nonblock-statement-body-position": "off",
    "import/first": "off",
    "quote-props": "off",
    "radix": "off"
    // "prettier/prettier": "warn", //TODO: See if this can be turned on after rest is done 
  }
}
