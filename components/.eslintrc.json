{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "rules": {
    "indent": "off", // TODO: Turn on
    // "quotes": "warn", // TODO: Turn on, single or double quotes?
    // "no-trailing-spaces": "warn", // TODO: turn on
    // "key-spacing": "warn", // TODO: Turn on
    // "keyword-spacing": "warn", // TODO: Turn on
    "spaced-comment": "off", // TODO: Turn on
    "comma-spacing": "off", // TODO: turn on
    "arrow-spacing": "off", // TODO: turn on
    "no-extra-semi": "off", //TODO Turn on
    "no-multi-spaces": "off", // TODO: turn on
    "camelcase": "off", // TODO: turn on
    "array-bracket-spacing": "off", // TODO: Turn on
    "template-curly-spacing": "off", // TODO: turn on
    "no-var": "off", // TODO: Turn on
    "no-redeclare": "off", // TODO: turn on
    "padded-blocks": "off", // TODO: turn on
    "no-multiple-empty-lines": "off", // TODO: Turn on
    "eol-last": "off", // TODO: Turn on
    "space-before-function-paren": "off", // TODO: Turn on
    "lines-between-class-members": "off", // TODO:  Turn on
    "implicit-arrow-linebreak": "off", // TODO: Turn on
    "prefer-arrow-callback": "off", // TODO: Maybe turn on
    "arrow-body-style": "off", // TODO: Maybe turn on
    "prefer-template": "off", // TODO: turn on
    "linebreak-style": "warn",
    "import/no-extraneous-dependencies": "off", // TODO: Turn this rule on
    "import/prefer-default-export": "off",
    "arrow-parens": "off", // TODO: Turn on
    "no-underscore-dangle": "off", // TODO: turn this rule on
    "import/no-unresolved": "off", // TODO: check if we can use plugin for this rule
    "no-return-assign": "off", // TODO: Turn on this rule
    "class-methods-use-this": "off", // TODO: Check if this can be turned on
    "array-callback-return": "off", // TODO: Check this if needed ["warn", { "checkForEach": false }]
    "no-loop-func": "warn", // check this
    "no-self-assign": "warn", // check this
    "consistent-return": "off",
    "no-unused-expressions": "off", // TODO: Turn on ?
    "no-restricted-syntax": "off", // TODO: check this - disables for-in related error
    "import/no-dynamic-require": "warn",
    "func-names": "off", // TODO: Turn this on? you shouldn't use unnamed functions
    "no-useless-escape": "warn",
    "prefer-destructuring": "off", // TODO: Turn this on?
    "comma-dangle": "off", // TODO: Turn on
    "object-curly-spacing": "off", // TODO: Turn on
    "semi": "off", // TODO: turn on
    "object-shorthand": "off", // TODO: Turn on
    "space-before-blocks": "off", // TODO: turn on
    "space-infix-ops": "off", // TODO: Turn on
    "object-curly-newline": "off", // TODO: Turn on
    "eqeqeq": "off", // TODO: Turn on
    "operator-linebreak": "off", // Maybe turn on
    "import/order": "off", // Maybe turn on
    "prefer-const": "off", // TODO: Turn on
    "wrap-iife": "off", // TODO: Maybe turn on
    "no-empty-pattern": "off", // TODO: Turn on
    "import/newline-after-import": "off", // TODO: Turn on
    "vars-on-top": "off", // TODO: turn on
    "no-shadow": "off", // TODO: maybe turn on
    "block-scoped-var": "off", // TODO: Turn on
    "brace-style": "off", // TODO: turn on
    "no-array-constructor": "off", // TODO: turn on
    "prefer-object-spread": "off", // TODO:  Turn on
    "no-nested-ternary": "off", // TODO: Turn on
    "no-confusing-arrow": "off", // TODO: turn on
    "block-spacing": "off", // TODO: turn on
    "space-in-parens": "off", // TODO: turn on
    "semi-spacing": "off", // TODO: Turn on
    "no-restricted-globals": "warn", // TODO: turn on
    "no-prototype-builtins": "warn", // TODO: turn on
    "radix": ["error", "as-needed"], // TODO: Turn on
    "max-len": "off", // TODO: Set limit ?
    "no-use-before-define": "off", //Turn Off
    "no-console": "off", // Turn off
    "no-undef": "off", //Turn off
    "function-paren-newline": "off", //Turn off
    "dot-notation": "off", // TODO: off
    "import/extensions": "off", // TODO: off for now
    "no-plusplus": "off", //Turn off
    "no-param-reassign": "off" // check this[ "off", { "props": false }]
    // "prettier/prettier": "warn" // TODO: See if this can be turned on after rest is done
  }
}
