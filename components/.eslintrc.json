{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": ["@typescript-eslint", "prettier"],
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    },
    "project": "./tsconfig.json"
  },
  "rules": {
    // "prettier/prettier": "warn" // TODO: See if this can be turned on after rest is done,
    "indent": "off", // TODO: Turn on
    "spaced-comment": "warn", // TODO: Turn on
    "camelcase": "warn", // TODO: turn on
    "no-var": "warn", // TODO: Turn on
    "lines-between-class-members": "warn", // TODO: turn on
    "implicit-arrow-linebreak": "warn", // TODO: turn on
    "prefer-arrow-callback": "warn", // TODO: turn on
    "arrow-body-style": "warn", // TODO: turn on
    "prefer-template": "warn", // TODO: turn on
    "import/no-extraneous-dependencies": "off", // TODO: Turn this rule on
    "import/prefer-default-export": "warn",
    "arrow-parens": "warn", // TODO: Turn on
    "no-underscore-dangle": "warn", // TODO: turn this rule on
    "import/no-unresolved": "warn", // TODO: check if we can use plugin for this rule
    "no-return-assign": "warn", // TODO: Turn on this rule
    "class-methods-use-this": "warn", // TODO: Check if this can be turned on
    "array-callback-return": "warn", // TODO: Check this if needed ["warn", { "checkForEach": false }]
    "consistent-return": "warn",
    "no-unused-expressions": "warn", // TODO: Turn on ?
    "import/no-dynamic-require": "warn",
    "prefer-destructuring": "warn", // TODO: Turn this on?
    "comma-dangle": "warn", // TODO: Turn on
    "object-shorthand": "warn", // TODO: Turn on
    "object-curly-newline": "warn", // TODO: Turn on
    "eqeqeq": "warn", // TODO: Turn on
    "operator-linebreak": "warn", // Maybe turn on
    "prefer-const": "warn", // TODO: Turn on
    "wrap-iife": "warn", // TODO: Maybe turn on
    "no-empty-pattern": "warn", // TODO: Turn on
    "import/newline-after-import": "warn", // TODO: Turn on
    "vars-on-top": "warn", // TODO: turn on
    "no-shadow": "warn", // TODO: maybe turn on
    "no-restricted-globals": "warn", // TODO: turn on
    "no-prototype-builtins": "warn", // TODO: turn on
    "radix": ["error", "as-needed"], // TODO: Turn on
    "max-len": "off", // TODO: Set limit ?
    "no-use-before-define": "off", //Turn Off
    "no-console": "off", // Turn off
    "no-undef": "off", //Turn off
    "function-paren-newline": "off", //Turn off
    "dot-notation": "off", // TODO: off
    "import/extensions": "off", // TODO: off for now
    "no-plusplus": "off", //Turn off
    "no-param-reassign": "off", // check this[ "off", { "props": false }]
    "linebreak-style": "off" // TODO: turn on
